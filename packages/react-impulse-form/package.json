  "keywords": [
    "react",
    "react-forms",
    "forms",
    "forms-state"
  ],
  "author": "Anton Ovechkin <owanturist@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/owanturist/react-impulse/issues"
  },
  "repository": "https://github.com/owanturist/react-impulse/tree/master/packages/react-impulse-form",
  "homepage": "https://github.com/owanturist/react-impulse/packages/react-impulse-form#readme",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup --config ../../tsup.config.ts"
  },
  "peerDependencies": {
    "react-impulse": "^3.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.12",
    "react": "^19.1.1",
    "react-dom": "19.1.1",
    "react-impulse": "workspace:*",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.5"
